  </formats>

  <fileSets>
    <fileSet>
      <directory>target/results/static_content</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>css/**/*</include>
        <include>js/**/*</include>
        <include>external/**/*</include>
        <include>images/**/*</include>
        <include>font/**/*</include>
        <include>themes/**/*</include>
        <include>errors/**/*</include>
        <include>/terms.html</include>
        <include>/webui.html</include>
      </includes>
    </fileSet>
  </fileSets>

</assembly>
