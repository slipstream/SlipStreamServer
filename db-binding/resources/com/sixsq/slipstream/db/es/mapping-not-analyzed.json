{
  "dynamic_templates": [
    {
      "strings": {
        "match": "*",
        "match_mapping_type": "string",
        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "longs": {
        "match": "*",
        "match_mapping_type": "long",
        "mapping": {
          "type": "long"
        }
      }
    }
  ]
}
