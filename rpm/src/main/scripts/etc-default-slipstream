export ES_HOST=localhost
export ES_PORT=9300
export JETTY_HOME=/opt/slipstream/server
export JETTY_RUN=/var/run
export JETTY_PID=$JETTY_RUN/slipstream.pid
export JETTY_USER=slipstream
export TMPDIR=/opt/slipstream/server/tmp
export JAVA=/etc/alternatives/jre_${server.jre.version}/bin/java
export PASSPHRASE=sl1pstre8m
export JETTY_ARGS="$JETTY_ARGS -Des.host=$ES_HOST -Des.port=$ES_PORT"

